{
  "name": "unplugin-quansync",
  "version": "0.4.1",
  "packageManager": "pnpm@10.10.0",
  "description": "Write async functions, get both async and sync functions",
  "type": "module",
  "keywords": [
    "async",
    "sync",
    "generator",
    "quansync",
    "unplugin",
    "vite",
    "webpack",
    "rspack",
    "rollup",
    "rolldown",
    "esbuild",
    "farm"
  ],
  "license": "MIT",
  "homepage": "https://github.com/quansync-dev/unplugin-quansync#readme",
  "bugs": {
    "url": "https://github.com/quansync-dev/unplugin-quansync/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quansync-dev/unplugin-quansync.git"
  },
  "author": "三咲智子 Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./vite": "./dist/vite.js",
    "./webpack": "./dist/webpack.js",
    "./rspack": "./dist/rspack.js",
    "./rollup": "./dist/rollup.js",
    "./rolldown": "./dist/rolldown.js",
    "./esbuild": "./dist/esbuild.js",
    "./farm": "./dist/farm.js",
    "./api": "./dist/api.js",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "quansync": ">=0.2.2"
  },
  "dependencies": {
    "ast-kit": "^2.0.0",
    "magic-string-ast": "^0.9.1",
    "unplugin": "^2.3.2"
  },
  "devDependencies": {
    "@babel/types": "^7.27.1",
    "@sxzz/eslint-config": "^7.0.0",
    "@sxzz/prettier-config": "^2.2.1",
    "@sxzz/test-utils": "^0.5.6",
    "@types/node": "^22.15.17",
    "bumpp": "^10.1.0",
    "eslint": "^9.26.0",
    "prettier": "^3.5.3",
    "quansync": "^0.2.10",
    "tsdown": "^0.11.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "prettier": "@sxzz/prettier-config"
}
